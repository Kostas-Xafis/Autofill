{
    "name": "form",
    "version": "1.0",
    "manifest_version": 2,
    "web_accessible_resources": [
        "db.json"
    ],
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": ["script.js"]
        }
    ],
    "permissions": ["tabs"],
    "background": {
        "scripts": ["background.js"]    
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Form Autofiller"
    }
}